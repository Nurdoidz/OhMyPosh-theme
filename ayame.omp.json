{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#17131e",
          "background": "#cd40c7",
          "leading_diamond": "",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}"
        },
        {
          "type": "session",
          "style": "diamond",
          "powerline_symbol": "",
          "foreground": "#17131e",
          "background": "#cd40c7",
          "trailing_diamond": "",
          "template": " {{ .HostName }} <#17131e> {{if .Root}} {{.UserName}} {{else}}{{.UserName}} {{end}}"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "powerline_symbol": "",
          "foreground": "#857abc",
          "background": "#2b284a",
          "template": "  {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          },
          "trailing_diamond": "",
          "leading_diamond": "<transparent,#2b284a></>"
        },
        {
          "type": "path",
          "foreground": "#17131e",
          "background": "#51a2e8",
          "powerline_symbol": "",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": " <#17131e></> ",
            "home_icon": "󰜥 ",
            "style": "agnoster_full"
          },
          "style": "powerline",
          "template": " {{ .Path }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#17131e",
          "background": "#5ee59d",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f5cb40{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e78d5b{{ end }}",
            "{{ if gt .Ahead 0 }}#ea5279{{ end }}",
            "{{ if gt .Behind 0 }}#7dd06b{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#ea5279{{ end }}"
          ],
          "trailing_diamond": "",
          "template": "  ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#17131e",
          "background": "#5ee59d",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 󰃫 {{ .Env.keki_cake }} <#17131e></> 󰌨 {{ .Env.keki_layers }} <#17131e></> {{ .Env.keki_layer }} "
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\u00a0\u00a0"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#857abc",
          "background": "#2b284a",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#17131e",
          "background": "#ea5279",
          "template": ""
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#17131e",
          "background": "#8852ea",
          "template": "  {{ .Name }} {{ .Env.POSH_INSTALER }}"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\u00a0"
        }
      ],
      "newline": true
    }
  ]
}
